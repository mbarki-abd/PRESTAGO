{
  "name": "@prestago/plugin-users",
  "version": "1.0.0",
  "description": "PRESTAGO - Users, Organizations & Roles Management Plugin",
  "license": "UNLICENSED",
  "main": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
